<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="../src/XMLInput/simulation.xsd">
  
  
  <parameter>
    <start-time>0</start-time>
    <end-time>5</end-time>
    <delta-t>0.0002</delta-t>
    
    <force-calculator>
      <type>lennard-jones</type>
      <epsilon>5</epsilon>
      <sigma>1</sigma>
    </force-calculator>
    
    <simulation-mode>
      <type>normal</type>
    </simulation-mode>
  </parameter>
  
  
  <input>
    <cuboid x1="0.0" x2="0.0" x3="0.0" n1="40" n2="80" n3="1" h="1.1225" m="1.0" v1="0.0" v2="0.0" v3="0.0" />
	<cuboid x1="15.0" x2="15.0" x3="0.0" n1="8" n2="8" n3="1" h="1.1225" m="1.0" v1="0.0" v2="-10.0" v3="0.0" />
  </input>
  
  
  <output>
    <base_filename>Output/MD_vtk</base_filename>
    <output_freq>10</output_freq>
  </output>
  
  
</simulation>

